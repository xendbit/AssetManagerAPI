{
  "name": "asset-manager-api",
  "version": "0.0.1",
  "description": "",
  "author": "Akintayo A. Olusegun",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prestart": "cp -r src/etc dist/src/",
    "prestart:dev": "npm run build && npm run prestart",
    "prestart:prod": "npm run prestart",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "npm run prestart",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev && nest start --watch",
    "start:debug": "NODE_ENV=dev && nest start --debug --watch",
    "start:prod": "NODE_ENV=prod && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "m:g": "typeorm migration:generate -n",
    "m:r": "ts-node ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/typeorm": "^7.1.4",
    "@types/bcrypt": "^3.0.0",
    "@types/crypto-js": "^4.0.1",
    "@types/web3": "^1.2.2",
    "bcrypt": "^5.0.0",
    "bip39": "^3.0.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cloudinary": "^1.23.0",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "ethereumjs-tx": "^2.1.2",
    "ethereumjs-vm": "^4.2.0",
    "ethereumjs-wallet": "^1.0.1",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^2.2.6",
    "nodemailer": "^6.4.17",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.4",
    "typed-rest-client": "^1.8.0",
    "typeorm": "^0.2.29",
    "web3": "^1.3.0",
    "web3-utils": "^1.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
